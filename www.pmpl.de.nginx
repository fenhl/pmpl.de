server {
    server_name www.pmpl.de;

    listen 80;
    listen [::]:80;

    return 301 https://pmpl.de$request_uri;
}

server {
    server_name www.pmpl.de;

    listen 443 ssl;
    listen [::]:443 ssl;

    ssl_certificate /etc/letsencrypt/live/webroot/fullchain.pem;
    ssl_certificate_key /etc/letsencrypt/live/webroot/privkey.pem;

    return 301 https://pmpl.de$request_uri;
}
